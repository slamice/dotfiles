execute pathogen#infect()
syntax on
filetype plugin indent on

set smartindent
set tabstop=4
set shiftwidth=4
set cmdheight=1
set expandtab
set nu
set relativenumber
set number
set background=dark

set background=dark
colorscheme solarized

let g:airline_theme = 'solarized'
let g:airline_powerline_fonts = 1
